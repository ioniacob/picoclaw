{
  "version": 2,
  "name": "picoclaw-dev",
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/api/whatsapp",
      "dest": "/api/whatsapp.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/index.html"
    },
    {
      "src": "/admin",
      "dest": "/admin/index.html"
    },
    {
      "src": "/",
      "dest": "/admin/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "development"
  }
}